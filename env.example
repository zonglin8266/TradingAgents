# TradingAgents Environment Configuration
# Copy this file to .env and fill in your API keys

# =============================================================================
# REQUIRED API KEYS
# =============================================================================

# FinnHub API Key (Required for financial data)
# Get your free API key at: https://finnhub.io/register
FINNHUB_API_KEY=your_finnhub_api_key_here

# LLM API Keys (At least one is required)
# Choose your preferred LLM provider and set the corresponding API key

# OpenAI API Key
# Get your API key at: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Google AI API Key (Alternative to OpenAI)
# Get your API key at: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Anthropic API Key (Alternative to OpenAI)
# Get your API key at: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# =============================================================================
# OPTIONAL CONFIGURATION
# =============================================================================

# Results Directory (where analysis results will be saved)
TRADINGAGENTS_RESULTS_DIR=./results

# Redis URL (for caching, uses docker-compose redis by default)
REDIS_URL=redis://redis:6379

# =============================================================================
# DOCKER CONFIGURATION
# =============================================================================

# Docker image tag
DOCKER_IMAGE_TAG=latest

# Container timezone
TZ=UTC

# =============================================================================
# LLM PROVIDER CONFIGURATION
# =============================================================================

# Default LLM provider (openai, google, anthropic)
LLM_PROVIDER=openai

# OpenAI specific settings
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL_DEEP=o4-mini
OPENAI_MODEL_QUICK=gpt-4o-mini

# Google AI specific settings
GOOGLE_BASE_URL=https://generativelanguage.googleapis.com/v1
GOOGLE_MODEL_DEEP=gemini-2.0-flash
GOOGLE_MODEL_QUICK=gemini-2.0-flash

# Anthropic specific settings
ANTHROPIC_BASE_URL=https://api.anthropic.com
ANTHROPIC_MODEL_DEEP=claude-3-5-sonnet-20241022
ANTHROPIC_MODEL_QUICK=claude-3-5-haiku-20241022

# =============================================================================
# TRADING CONFIGURATION
# =============================================================================

# Maximum debate rounds for research team
MAX_DEBATE_ROUNDS=1

# Maximum risk discussion rounds
MAX_RISK_DISCUSS_ROUNDS=1

# Enable online tools (true/false)
ONLINE_TOOLS=true

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================

# Enable debug mode
DEBUG=true

# Python log level
PYTHON_LOG_LEVEL=INFO

# Enable Jupyter notebook service
ENABLE_JUPYTER=false